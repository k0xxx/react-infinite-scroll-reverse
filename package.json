  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-react": "^7.12.7",
    "node-sass": "^7.0.0",
    "prettier": "2.2.1",
    "react-scripts": "4.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
